#include <stdio.h>
// exercicio 04

int main() {
    int num;
    int x;

    printf("Digite um número inteiro: ");
    scanf("%d", &num);

    printf("Os divisores de %d são: ", num);
    for (x = 1; x <= num; x++) {
        if (num % x == 0) {
            printf("%d ", x);
        }
    }
    printf("\n");

    return 0;
}
